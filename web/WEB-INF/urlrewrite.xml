<urlrewrite>  
    <rule>
        <from>/usuario/(.*)$</from>
        <to type="redirect">/Chuirer/public.do?$1</to>
    </rule> 
    <rule>
        <from>/mensaje/(.*)$</from>
        <to type="forward">/Chuirer/message.do?$1</to>
    </rule>
</urlrewrite>  